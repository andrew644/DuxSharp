fn main() -> i32 {
    printf("7 + 88 = %d\n", add(7, 88))
    for i := 1, i <= 30, i += 1 {
        printf("fibonacci(%d) = %d\n", i, fib(i))
    }
    return 0
}

fn add(a: i32, b: i32) -> i32 {
    return a + b
}

fn fib(k: i32) -> i32 {
    if k == 1 or k == 0 {
        return 1
    }
    
    return fib(k - 1) + fib(k - 2)
}