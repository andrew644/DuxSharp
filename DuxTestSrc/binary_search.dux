fn main() -> i32 {

	nums: [10]i32
	nums[0] = 1
	nums[1] = 3
	nums[2] = 4
	nums[3] = 6
	nums[4] = 9
	nums[5] = 10
	nums[6] = 14
	nums[7] = 22
	nums[8] = 36
	nums[9] = 100

	target := 4
	
	left := 0
	right := 10 - 1
	mid: i32
	for left <= right {
		mid = left + (right - left) / 2
		if nums[mid] < target {
			left = mid + 1
		} else if nums[mid] > target {
			right = mid - 1
		} else {
			printf("found:%d\n", mid)
			return 0
		}
	}

	printf("we would insert into:%d\n", left)

	return 0
}
