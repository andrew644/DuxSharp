fn main() -> i32 {
	arr: [8]i32
	for i := 0, i < 8, i += 1 {
		arr[i] = i + 2
		printf("%d\n", arr[i])
	}
	arr[2] = 5
	printf("%d\n", arr[2])

	s := sum(arr, 8)
	printf("array sum:%d\n", s)

	return 0
}

fn sum(arr: []i32, size: i32) -> i32 {
	sum := 0
	for i := 0, i < size, i += 1 {
		sum += arr[i]
	}
	return sum
}
